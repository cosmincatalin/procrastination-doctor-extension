{
  "name": "Mooment",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Monitor the time you spend on the different websites.",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/common/namespace.js",
      "js/common/jquery.js",
      "js/backend/user.js",
      "js/backend/cors.js",
      "js/backend/util.js",
      "js/backend/host.js",
      "js/backend/monitor.js",
      "js/backend/routes.js",
      "js/backend/bootstrap.js"
    ],
    "persistent": false
  }
}