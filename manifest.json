{
  "name": "Procrastination Doctor",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": ".",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
    },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "Procrastination Doctor"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs",
    "idle"
  ],
  "background": {
    "scripts": [
      "js/common/namespace.js",
      "js/common/jquery.js",
      "js/backend/user.js",
      "js/backend/cors.js",
      "js/backend/util.js",
      "js/backend/host.js",
      "js/backend/data.js",
      "js/backend/monitor.js",
      "js/backend/routes.js",
      "js/backend/bootstrap.js"
    ],
    "persistent": true
  }
}